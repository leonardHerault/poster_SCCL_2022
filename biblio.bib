	keywords = {Cell commitment, Gene-regulatory network, Logical/Boolean model, Model checking, Notch signaling, Stochastic simulation, T cell development, Thymus},
	pages = {205--238},
	file = {ScienceDirect Full Text PDF:/home/leonard/Zotero/storage/SHTLDWGW/Cacace et al. - 2020 - Chapter Seven - Logical modeling of cell fate spec.pdf:application/pdf;ScienceDirect Snapshot:/home/leonard/Zotero/storage/L9KSNQC6/S007021532030034X.html:text/html},
