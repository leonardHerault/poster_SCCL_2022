	note = {Number: 6},
	keywords = {Animals, Humans, Epigenomics, Hematopoietic Stem Cells, Chromatin, Hematopoiesis, hematopoiesis, single cell, Cell Differentiation, Epigenesis, Genetic, Myeloid Progenitor Cells, Regulatory Sequences, Nucleic Acid, epigenomics, Cell Lineage, Transcriptome, Single-Cell Analysis, Transcription Factors, Sequence Analysis, RNA, Gene Expression Regulation, Hematopoietic Stem Cell Transplantation, chromatin accessibility, Principal Component Analysis},
