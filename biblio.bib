	note = {Number: 7110},
	keywords = {Animals, Mice, Aging, Cellular Senescence, Hematopoietic Stem Cells, Cell Proliferation, Bone Marrow Transplantation, Apoptosis, Basic Helix-Loop-Helix Transcription Factors, Cell Count, Cyclin-Dependent Kinase Inhibitor p16, Homeodomain Proteins, Transcription Factor HES-1},
	pages = {421--426},
}

