	note = {Number: 26},
	keywords = {leukemia, lineage potential, microarray, ontogeny},
	pages = {9194--9199},
	file = {Full Text PDF:/home/leonard/Zotero/storage/VXD5BZW3/Rossi et al. - 2005 - Cell intrinsic alterations underlie hematopoietic .pdf:application/pdf;Snapshot:/home/leonard/Zotero/storage/BUZ4LHLA/9194.html:text/html},
