	author = {Dugourd, Aurelien and Saez-Rodriguez, Julio},
	month = jun,
	year = {2019},
	keywords = {Transcriptomics, Integration, Multi-omics, Metabolomics, Data analysis, Footprint, Functional, Mechanistic, Phosphoproteomics, Proteomics, Trans-omics},
	pages = {82--90},
}

