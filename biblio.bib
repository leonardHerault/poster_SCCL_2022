	keywords = {Animals, Gene Expression Profiling, Humans, Hematopoietic Stem Cells, Clone Cells, Cell Differentiation, Cell Lineage, Mice, Inbred C57BL, Single-Cell Analysis, Cell Proliferation, Gene Expression Regulation, Genome, Hematopoietic Stem Cell Transplantation},
