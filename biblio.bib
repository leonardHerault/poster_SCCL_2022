	year = {2020},
	pages = {193--209},
	file = {HAL PDF Full Text:/home/leonard/Zotero/storage/7PXUSKAM/Chevalier et al. - 2020 - Synthesis and Simulation of Ensembles of Boolean N.pdf:application/pdf},
}

