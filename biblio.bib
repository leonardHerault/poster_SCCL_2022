	note = {Number: 11},
	keywords = {Software, Algorithms, Neural Networks, Computer, Phenotype, Models, Biological, Computational Biology, Stochastic Processes, Archaeal Proteins, Bacteriophage lambda, Halobacterium, Internet, Software Design},
	pages = {2498--2504},
}

