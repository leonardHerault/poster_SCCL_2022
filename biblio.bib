@article{nitta_bmi1_2020-5,
	title = {Bmi1 counteracts hematopoietic stem cell aging by repressing target genes and enforcing the stem cell gene signature},
	volume = {521},
	issn = {1090-2104},
	doi = {10.1016/j.bbrc.2019.10.153},
	abstract = {Polycomb-group proteins are critical regulators of stem cells. We previously demonstrated that Bmi1, a component of polycomb repressive complex 1, defines the regenerative capacity of hematopoietic stem cells (HSCs). Here, we attempted to ameliorate the age-related decline in HSC function by modulating Bmi1 expression. The forced expression of Bmi1 did not attenuate myeloid-biased differentiation of aged HSCs. However, single cell transplantation assays revealed that the sustained expression of Bmi1 augmented the multi-lineage repopulating capacity of aged HSCs. Chromatin immunoprecipitation-sequencing of Bmi1 combined with an RNA sequence analysis showed that the majority of Bmi1 direct target genes are developmental regulator genes marked with a bivalent histone domain. The sustained expression of Bmi1 strictly maintained the transcriptional repression of their target genes and enforced expression of HSC signature genes in aged HSCs. Therefore, the manipulation of Bmi1 expression is a potential approach against impairments in HSC function with aging.},
	language = {eng},
	number = {3},
	journal = {Biochem Biophys Res Commun},
	author = {Nitta, Eriko and Itokawa, Naoki and Yabata, Shogo and Koide, Shuhei and Hou, Li-Bo and Oshima, Motohiko and Aoyama, Kazumasa and Saraya, Atsunori and Iwama, Atsushi},
	month = jan,
	year = {2020},
	pmid = {31679686},
	note = {Number: 3},
	keywords = {Animals, Mice, Aging, Cellular Senescence, Hematopoietic Stem Cells, Hematopoiesis, Myeloid Cells, Hematopoietic stem cells, Gene Expression Regulation, Bmi1, Polycomb repressive complex, Polycomb Repressive Complex 1, Proto-Oncogene Proteins},
	pages = {612--619},
}

@article{de_haan_epigenetic_2007-5,
	title = {Epigenetic control of hematopoietic stem cell aging the case of {Ezh2}},
	volume = {1106},
	issn = {0077-8923},
	doi = {10.1196/annals.1392.008},
	abstract = {Hematopoietic stem cells have potent, but not unlimited, selfrenewal potential. The mechanisms that restrict selfrenewal are likely to play a role during aging. Recent data suggest that the regulation of histone modifications by Polycomb group genes may be of crucial relevance to balance selfrenewal and aging. We provide evidence for the involvement of one of these Polycomb group genes, Ezh2, in aging of the hematopoietic stem cell system.},
