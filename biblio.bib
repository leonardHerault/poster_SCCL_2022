	keywords = {Boolean networks, Computational systems biology, model analysis, Python programming language, reproducibility},
	file = {Full Text PDF:/home/leonard/Zotero/storage/UYERDMQB/Naldi et al. - 2018 - The CoLoMoTo Interactive Notebook Accessible and .pdf:application/pdf},
