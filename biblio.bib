	pages = {1737369},
	file = {Full Text PDF:/home/leonard/Zotero/storage/A4AEW6QE/Carmona et al. - 2020 - Deciphering the transcriptomic landscape of tumor-.pdf:application/pdf;Snapshot:/home/leonard/Zotero/storage/V3AKKLRB/2162402X.2020.html:text/html},
