	note = {Number: 21},
	keywords = {Animals, Cells, Cultured, Gene Expression Profiling, Mice, Mutation, Hematopoietic Stem Cells, Cell Differentiation, Bone Marrow Cells, Cell Lineage, Transcriptome, Single-Cell Analysis, Signal Transduction, Mice, Knockout, Proto-Oncogene Proteins c-kit, Cell Line, Tumor},
	pages = {e1--e11},
}

