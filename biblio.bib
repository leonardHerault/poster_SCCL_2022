	pmcid = {PMC4070311},
	keywords = {Animals, Cells, Cultured, Mice, Cellular Senescence, Epigenomics, Hematopoietic Stem Cells, Male, Cell Differentiation, Chromatin Immunoprecipitation, Transforming Growth Factor beta},
	pages = {673--688},
	file = {Texte intégral:/home/leonard/Zotero/storage/W3WQCFNT/Sun et al. - 2014 - Epigenomic profiling of young and aged HSCs reveal.pdf:application/pdf},
