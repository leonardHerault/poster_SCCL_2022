	author = {Kirouac, Daniel C and Madlambayan, Gerard J and Yu, Mei and Sykes, Edward A and Ito, Caryn and Zandstra, Peter W},
	month = jan,
	year = {2009},
	note = {Publisher: John Wiley \& Sons, Ltd},
	keywords = {hematopoiesis, stem cells, cell culture, cellular networks, modelling},
	pages = {293},
	file = {Snapshot:/home/leonard/Zotero/storage/PTD4TZKB/msb.2009.html:text/html;Texte intégral:/home/leonard/Zotero/storage/KNFWI2H8/Kirouac et al. - 2009 - Cell–cell interaction networks regulate blood stem.pdf:application/pdf},
