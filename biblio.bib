	keywords = {Animals, Mice, Cellular Senescence, Hematopoietic Stem Cells, DNA Damage, Female, Male, Histones, Mice, Inbred C57BL, Cell Proliferation, Gene Expression Regulation, Stress, Physiological, DNA Replication, DNA, Ribosomal, Minichromosome Maintenance Proteins},
	pages = {198--202},
	file = {Texte intégral:/home/leonard/Zotero/storage/V6L2PF2C/Flach et al. - 2014 - Replication stress is a potent driver of functiona.pdf:application/pdf},
