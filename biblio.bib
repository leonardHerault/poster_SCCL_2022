	pages = {796--804},
	file = {PubMed Central Full Text PDF:/home/leonard/Zotero/storage/2655CU2H/Marbach et al. - 2012 - Wisdom of crowds for robust gene network inference.pdf:application/pdf},
}

@article{mann_heterogeneous_2018,
	title = {Heterogeneous {Responses} of {Hematopoietic} {Stem} {Cells} to {Inflammatory} {Stimuli} {Are} {Altered} with {Age}},
