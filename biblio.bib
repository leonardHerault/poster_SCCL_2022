	urldate = {2021-05-26},
	journal = {Experimental Hematology},
	author = {Hay, Stuart B. and Ferchen, Kyle and Chetal, Kashish and Grimes, H. Leighton and Salomonis, Nathan},
	month = dec,
	year = {2018},
	pages = {51--61},
}

@article{calo_modification_2013-1,
	title = {Modification of {Enhancer} {Chromatin}: {What}, {How}, and {Why}?},
