	pmcid = {PMC528895},
	pages = {2747--2763},
	file = {PubMed Central Full Text PDF:/home/leonard/Zotero/storage/2FB7SG5E/Wilson et al. - 2004 - c-Myc controls the balance between hematopoietic s.pdf:application/pdf},
}

