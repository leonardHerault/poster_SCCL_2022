	note = {Number: 3},
	keywords = {Animals, Gene Expression Profiling, Hematopoietic Stem Cells, Female, Male, Cell Lineage, Mice, Inbred C57BL, Single-Cell Analysis, Transcription Factors, Sequence Analysis, RNA, Gene Expression Regulation, Transcription Factor HES-1, Anemia, Erythropoiesis},
