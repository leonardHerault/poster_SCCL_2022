	month = nov,
	year = {2017},
	pmid = {28216661},
	pmcid = {PMC5668498},
	pages = {2315--2325},
	file = {PubMed Central Full Text PDF:/home/leonard/Zotero/storage/DFZDW4TL/Dell'Aversana et al. - 2017 - miR-194-5pBCLAF1 deregulation in AML tumorigenesi.pdf:application/pdf},
}

