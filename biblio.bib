	note = {Number: 7437},
	keywords = {Animals, Mice, Aging, Cellular Senescence, Hematopoietic Stem Cells, Mice, Inbred C57BL, Gene Expression Regulation, Cell Survival, Stress, Physiological, Apoptosis, Homeostasis, Autophagy, Caloric Restriction, Cytokines, Energy Metabolism, Food Deprivation, Forkhead Box Protein O3, Forkhead Transcription Factors},
	pages = {323--327},
}

@article{adolfsson_identification_2005-8,
	title = {Identification of {Flt3}+ {Lympho}-{Myeloid} {Stem} {Cells} {Lacking} {Erythro}-{Megakaryocytic} {Potential}: {A} {Revised} {Road} {Map} for {Adult} {Blood} {Lineage} {Commitment}},
